<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE struts PUBLIC 
    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
    "http://struts.apache.org/dtds/struts-2.0.dtd">

<struts>
	<package name="logon" namespace="/logon" extends="struts_base">
		<action name="toLogon" class="com.vms.logon.action.LogonAction" method="toLogon">
            <result name="success">logon.jsp</result>
        </action>
        
		<action name="doLogon" class="com.vms.logon.action.LogonAction" method="doLogon">
            <result name="success">ok.jsp</result>
            <result name="input">logon.jsp</result>
        </action>
	</package>
</struts>