<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping package="com.vms.db.bean">
	<joined-subclass name="Auditing" table="auditing"
		extends="Vediotape">
		<meta attribute="sync-DAO">true</meta>
		<key column="vedioID" />
		<property name="auditDate" column="auditDate" type="timestamp"
			not-null="true" length="19" />
		<property name="comments" column="comments" type="string"
			not-null="true" length="512" />
		<many-to-one name="result" column="result" class="Status"
			not-null="true">
		</many-to-one>
		<many-to-one name="auditor" column="auditor" class="User"
			not-null="true">
		</many-to-one>
	</joined-subclass>
</hibernate-mapping>