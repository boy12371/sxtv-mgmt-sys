<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="com.vms.db.bean">
	<class name="Scorelevel" table="scorelevel">
		<meta attribute="sync-DAO">true</meta>
		<id name="id" type="integer" column="id">
			<generator class="native"/>
		</id>

		<property name="level" column="level" type="integer" not-null="true" length="10"/>
		<property name="start" column="start" type="java.lang.Float" not-null="true" length="12"/>
		<property name="end" column="end" type="java.lang.Float" not-null="true" length="12"/>
		<property name="comments" column="comments" type="string"
			not-null="false" length="512" />

	</class>	
</hibernate-mapping>