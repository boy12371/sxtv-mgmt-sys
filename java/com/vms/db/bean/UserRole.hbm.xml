<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="com.vms.db.bean">
	<class name="UserRole" table="user_role">
		<meta attribute="sync-DAO">false</meta>
		<composite-id name="Id" class="UserRolePK">
			<key-many-to-one name="Roleid" class="Role"	column="roleid"/>
			<key-many-to-one name="Userid" class="User"	column="userid"/>
		</composite-id>
		<property name="Comments" column="comments"	type="string" not-null="true" length="512"/>
	</class>	
</hibernate-mapping>