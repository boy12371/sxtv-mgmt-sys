<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="com.vms.db.bean">
	<class name="Audiencescore" table="audiencescore">
		<meta attribute="sync-DAO">true</meta>
		<composite-id name="Id" class="AudienceScorePK">
			<key-many-to-one name="audienceID"	class="Audience" column="audienceID"/>
			<key-many-to-one name="vedioID" class="Vediotape" column="vedioID"/>
		</composite-id>
		<property name="dateExamine" column="dateExamine" type="timestamp" not-null="true" length="19"/>
		<property name="result" column="result" type="integer" not-null="true" length="10"/>
		<property name="comments" column="comments" type="string" not-null="false" length="512"/>
	</class>	
</hibernate-mapping>
