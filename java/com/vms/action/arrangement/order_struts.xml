<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
"-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
"http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
	<package name="order" namespace="/order" extends="json-default">
		<action name="toArrangePlayOrder" class="orderMgmt" method="toArrangePlayOrder">
			<result>/order/videoOrder.jsp</result>
		</action>
		<action name="createPlayorder" class="orderMgmt" method="createPlayorder">
			<result>/order/playOrders.jsp</result>
			<result name="input">/order/videoOrder.jsp</result>			
		</action>
		
		<action name="getOrderListForMonth" class="orderMgmt" method="getOrderListForMonth">
			<result type="json">
				<param name="root">table</param>
			</result>
		</action>

	</package>
</struts>