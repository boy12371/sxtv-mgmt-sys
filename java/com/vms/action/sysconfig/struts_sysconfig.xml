<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
"-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
"http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
	<include file="struts-default.xml" />


	
	<package name="logon" namespace="/sys" extends="json-default">
    	
		<action name="toEmployees" class="com.vms.action.sysconfig.EmployeeMgmtAction" method="toEmployees">
			<result name="success">/sysconfig/employees.jsp</result>
		</action>       
        
		<action name="toAddEmployee" class="com.vms.action.sysconfig.EmployeeMgmtAction" method="toAddEmployee">
			<result name="success">/sysconfig/addEmployee.jsp</result>
		</action>
        
		<action name="doAddEmployee" class="com.vms.action.sysconfig.EmployeeMgmtAction" method="doAddEmployee">
			<result type="chain">
				<param name="actionName">toEmployees</param>
			</result>
			<result type="input">/sysconfig/addEmployee.jsp</result>
		</action>
        
	</package>
  

</struts>

